# THE OVERMIND PROTOCOL - Environment Configuration
# Copy this file to .env and fill in your actual values

# ============================================================================
# TRADING CONFIGURATION
# ============================================================================
SNIPER_TRADING_MODE=paper
OVERMIND_MODE=enabled
OVERMIND_AI_MODE=enabled
OVERMIND_AI_CONFIDENCE_THRESHOLD=0.7

# Risk Management
SNIPER_MAX_POSITION_SIZE=10000
SNIPER_MAX_DAILY_LOSS=1000

# ============================================================================
# API KEYS (REQUIRED)
# ============================================================================

# OpenAI API Key (REQUIRED for AI Brain)
OPENAI_API_KEY=your-openai-api-key

# Optional AI Providers
GROQ_API_KEY=your-groq-api-key
MISTRAL_API_KEY=your-mistral-api-key
GOOGLE_API_KEY=your-google-api-key
PERPLEXITY_API_KEY=your-perplexity-api-key

# Financial Data
FINANCIAL_DATASETS_API_KEY=your-financial-datasets-api-key

# ============================================================================
# SOLANA CONFIGURATION
# ============================================================================

# Devnet (for testing)
SOLANA_DEVNET_RPC_URL=https://distinguished-blue-glade.solana-devnet.quiknode.pro/a10fad0f63cdfe46533f1892ac720517b08fe580
SOLANA_DEVNET_WSS_URL=wss://distinguished-blue-glade.solana-devnet.quiknode.pro/a10fad0f63cdfe46533f1892ac720517b08fe580

# Mainnet (for live trading)
SOLANA_MAINNET_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_MAINNET_WSS_URL=wss://api.mainnet-beta.solana.com

# API Keys
HELIUS_API_KEY=your-helius-api-key
QUICKNODE_API_KEY=your-quicknode-api-key

# Wallet Configuration (KEEP SECURE!)
SOLANA_WALLET_PRIVATE_KEY=your-wallet-private-key-base58

# ============================================================================
# DATABASE PASSWORDS (AUTO-GENERATED DURING DEPLOYMENT)
# ============================================================================
SNIPER_DB_PASSWORD=auto-generated-during-deployment
TENSORZERO_DB_PASSWORD=auto-generated-during-deployment
REDIS_PASSWORD=auto-generated-during-deployment
GRAFANA_ADMIN_PASSWORD=auto-generated-during-deployment

# ============================================================================
# PERFORMANCE SETTINGS
# ============================================================================
RUST_LOG=info
TOKIO_WORKER_THREADS=6
PYTHONPATH=/app/src

# ============================================================================
# MONITORING CONFIGURATION
# ============================================================================
PROMETHEUS_RETENTION=30d
GRAFANA_ALLOW_SIGN_UP=false

# ============================================================================
# DEVELOPMENT SETTINGS
# ============================================================================
# Uncomment for development
# RUST_LOG=debug
# OVERMIND_DEBUG_MODE=true
